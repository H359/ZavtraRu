{% extends "base.jhtml" %}

{% block app_header %}
<div id="headerbase">
  <div id="header">
    <div class="container12">
      <div class="row">
        {% block header %}{% endblock %}
      </div>
    </div>
  </div>
</div>
{% endblock %}

{% block app_content %}
<div id="page">{% block content %}{% endblock %}</div>
{% endblock %}

